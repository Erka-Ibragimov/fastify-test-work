{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "lib": [
        "es2020"
      ],  
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist"
    },
    "include": ["src/**/*.ts", "redis.ts"],
    "exclude": ["node_modules"]
  }
  